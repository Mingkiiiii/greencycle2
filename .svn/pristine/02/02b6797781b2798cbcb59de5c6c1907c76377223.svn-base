package com.greencycle.my.reply.VO;

public class ReplyVO {
	
	private int reNo;
	private int salesNo;
	private int reGrp;
	private int reGrps;
	private int reGrp1;
	private String userId;
	private String reContent;
	private String reDate;
	public int getReNo() {
		return reNo;
	}
	public void setReNo(int reNo) {
		this.reNo = reNo;
	}
	public int getSalesNo() {
		return salesNo;
	}
	public void setSalesNo(int salesNo) {
		this.salesNo = salesNo;
	}
	public int getReGrp() {
		return reGrp;
	}
	public void setReGrp(int reGrp) {
		this.reGrp = reGrp;
	}
	public int getReGrps() {
		return reGrps;
	}
	public void setReGrps(int reGrps) {
		this.reGrps = reGrps;
	}
	public int getReGrp1() {
		return reGrp1;
	}
	public void setReGrp1(int reGrp1) {
		this.reGrp1 = reGrp1;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getReContent() {
		return reContent;
	}
	public void setReContent(String reContent) {
		this.reContent = reContent;
	}
	public String getReDate() {
		return reDate;
	}
	public void setReDate(String reDate) {
		this.reDate = reDate;
	}
	@Override
	public String toString() {
		return "ReplyVO [reNo=" + reNo + ", salesNo=" + salesNo + ", reGrp=" + reGrp + ", reGrps=" + reGrps
				+ ", reGrp1=" + reGrp1 + ", userId=" + userId + ", reContent=" + reContent + ", reDate=" + reDate + "]";
	}
}
	
	
