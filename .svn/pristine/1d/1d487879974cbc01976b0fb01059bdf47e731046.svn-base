}
.character-img.character {
	animation: moveLeftRight 6s infinite; /* 좌우로 이동하는 애니메이션 적용 */
}

.character-img.character.eat {
	animation: moveUpDown 0.5s ease; /* 아이템을 먹었을 때 위아래로 이동하는 애니메이션 적용 */
}

/* 위아래로 흔들리는 애니메이션 정의 */
