}

.arrow-icon {
	width: 50px !important;
	height: 50px !important;
	cursor: pointer;
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	z-index: 10; /* 가장 앞부분에 표시되도록 z-index 설정 */
