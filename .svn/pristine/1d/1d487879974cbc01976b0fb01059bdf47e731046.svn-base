	}
	h1.mobile-title {
		display: block;
	}
	.select-button {
		margin-top: -14.7%;
		cursor: pointer;
		width: 80% !important;
		transform: translateY(98%);
	}
	.arrow-icon.left {
		margin-left: -48%;
	}
	.arrow-icon.right {
		margin-left: 36.5%;
	}
	h1.mobile-title {
		display: none;
	}
	.character-selection-content form {
		display: flex;
		justify-content: center;
		flex-wrap: nowrap;
		overflow: hidden;
		width: 100px;
		justify-content: space-evenly;
